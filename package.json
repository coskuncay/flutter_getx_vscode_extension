{
 "name": "fffs",
 "displayName": "Flutter GetX Module Generator",
 "description": "Flutter Feature First Structure GetX Module Generator",
 "version": "0.1.0",
 "publisher": "justecdev",
 "engines": {
  "vscode": "^1.56.0"
 },
 "bugs": {
  "url": "https://github.com/coskuncay/flutter_getx_vscode_extension/issues",
  "email": "emrecoskuncay@gmail.com"
 },
 "repository": {
  "type": "git",
  "url": "https://github.com/coskuncay/flutter_getx_vscode_extension"
 },
 "categories": [
  "Snippets",
  "Programming Languages"
 ],
 "activationEvents": [
  "onCommand:fffs.generateResource"
 ],
 "main": "./dist/extension.js",
 "contributes": {
  "commands": [
   {
    "command": "fffs.generateResource",
    "title": "FFFS: Generate GetX Module"
   } 
  ],
  "menus": {
    "explorer/context": [
        {
            "command": "fffs.generateResource",
            "title": "FFFS: Generate GetX Module",            
            "when": "explorerResourceIsFolder"
           }
    ]
  }
 },
 "scripts": {
  "vscode:prepublish": "npm run package",
  "compile": "webpack",
  "watch": "webpack --watch",
  "package": "webpack --mode production --devtool hidden-source-map",
  "compile-tests": "tsc -p . --outDir out",
  "watch-tests": "tsc -p . -w --outDir out",
  "pretest": "npm run compile-tests && npm run compile && npm run lint",
  "lint": "eslint src --ext ts",
  "test": "node ./out/test/runTest.js"
 },
 "devDependencies": {
  "@types/vscode": "^1.56.0",
  "@types/glob": "^8.0.0",
  "@types/mocha": "^10.0.0",
  "@types/node": "16.x",
  "@typescript-eslint/eslint-plugin": "^5.38.1",
  "@typescript-eslint/parser": "^5.38.1",
  "eslint": "^8.24.0",
  "glob": "^8.0.3",
  "mocha": "^10.0.0",
  "typescript": "^4.8.4",
  "ts-loader": "^9.4.1",
  "webpack": "^5.74.0",
  "webpack-cli": "^4.10.0",
  "@vscode/test-electron": "^2.1.5"
 },
 "dependencies": {
  "@types/js-yaml": "^4.0.1",
  "@types/lodash": "^4.14.169",
  "@types/mkdirp": "^1.0.1",
  "@types/rimraf": "^3.0.0",
  "@types/semver": "^7.3.6",
  "change-case": "^4.1.2"
 }
}
